{
    "name": "axettone/aweber-for-laravel",
    "type": "library",
    "description": "Aweber for Laravel is a PHP library that wraps AWeber APIs and let you connect your Larave",
    "keywords": ["aweber","marketing automation","laravel"],
    "license": "MIT",
    "authors": [{
        "name": "Paolo Niccol√≤ Giubelli"
    }],
    "require": {
        "aweber/aweber": "*"
    },
    "autoload": {
        "psr-4": {
            "AWeberForLaravel\\": "src/AWeber",
            "AWeberForLaravel\\Providers\\": "src/AWeber/Providers"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AWeberForLaravel\\Test\\": "test/AWeber"
        }
    }
